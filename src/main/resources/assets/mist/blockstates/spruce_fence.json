{
    "multipart": [
        {   "when": { "size": 0 },
            "apply": { "model": "mist:spruce_log_4" }
        },
        {   "when": { "size": 0, "deb": "true" },
            "apply": { "model": "mist:spruce_log_4_d" }
        },
        {   "when": { "size": 1 },
            "apply": { "model": "mist:spruce_log_8" }
        },
        {   "when": { "size": 1, "deb": "true" },
            "apply": { "model": "mist:spruce_log_8_d" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "mist:spruce_fence", "y": 90 }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "mist:spruce_fence", "y": 180 }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "mist:spruce_fence", "y": 270 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "mist:spruce_fence" }
        },
        {   "when": { "north": "false", "east": "false", "south": "false", "west": "false" },
            "apply": { "model": "mist:spruce_fence", "y": 90 }
        },
        {   "when": { "north": "false", "east": "false", "south": "false", "west": "false" },
            "apply": { "model": "mist:spruce_fence", "y": 180 }
        },
        {   "when": { "north": "false", "east": "false", "south": "false", "west": "false" },
            "apply": { "model": "mist:spruce_fence", "y": 270 }
        },
        {   "when": { "north": "false", "east": "false", "south": "false", "west": "false" },
            "apply": { "model": "mist:spruce_fence" }
        }
    ]
}